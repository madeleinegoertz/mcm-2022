@article{vanerp2020,
  title = {Case {{Report}}: {{Load}}, {{Intensity}}, and {{Performance Characteristics}} in {{Multiple Grand Tours}}},
  shorttitle = {Case {{Report}}},
  author = {VAN Erp, Teun and Hoozemans, Marco and Foster, Carl and DE Koning, Jos J.},
  date = {2020-04},
  journaltitle = {Medicine and Science in Sports and Exercise},
