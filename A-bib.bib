  author = {Felippe, Leandro Camati and Ferreira, Guilherme Assun\c{c}\~ao and Learsi, Sara Kely and Boari, Daniel and Bertuzzi, Romulo and Lima-Silva, Adriano Eduardo},
  date = {2018-06},
  journaltitle = {Journal of Applied Physiology},
