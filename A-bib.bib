  shorttitle = {Functional {{Threshold Power}} in {{Cyclists}}},
  author = {Borszcz, Fernando Klitzke and Tramontin, Artur Ferreira and Bossi, Arthur Henrique and Carminatti, Lorival Jos\'e and Costa, Vitor Pereira},
  date = {2018-10},
